<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <id>com.h8000572003.intellijplugin</id>

    <name>Values</name>
    <vendor email="wa.addi@gmail.com" url="https://github.com/h8000572003">AndyTsai</vendor>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService
                serviceImplementation="com.h8000572003.values.configurable.MyPluginSettings"></applicationService>
        <applicationConfigurable
                parentId="tools"
                instance="com.h8000572003.values.configurable.ValueConfigurable"
                id="com.h8000572003.values.configurable.ValueConfigurable"
                displayName="Values Settings"/>

        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateAllSetterFieldNameAction</className>
            <category>Java/Other</category>
        </intentionAction>
        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateUnionAssertAction</className>
            <category>Java/Other</category>
        </intentionAction>

        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateSourceMapperAction</className>
            <category>Java/Other</category>
        </intentionAction>

        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateSourceMapperV2Action</className>
            <category>Java/Other</category>
        </intentionAction>

        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateAllSetterNoCacheNameAction</className>
            <category>Java/Other</category>
        </intentionAction>
        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateAllByMethodAction</className>
            <category>Java/Other</category>
        </intentionAction>
        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateMapFromStringWithoutDeclareAction</className>
            <category>Java/Other</category>
        </intentionAction>
        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.GenerateMapFromStringWithDeclareAction</className>
            <category>Java/Other</category>
        </intentionAction>
        <intentionAction>
            <language>JAVA</language>
            <className>com.h8000572003.values.AddNullCheckAction</className>
            <category>Java/Other</category>
        </intentionAction>
    </extensions>
    <actions>

    </actions>
</idea-plugin>